{
  "expo": {
    "name": "3800gitHub",
    "slug": "3800gitHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/react-generated/icon.png",
    "splash": {
      "image": "./assets/react-generated/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/react-generated/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon/favicon.png"
    }
  }
}
